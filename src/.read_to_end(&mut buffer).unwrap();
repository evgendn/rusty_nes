use std::fs::File;
use std::io::Read;
use std::io::Error;
use std::path::Path;
use std::result::Result;

pub fn read_bin<P: AsRef<Path>>(path: P) -> Vec<u8> {
    let mut buffer: Vec<u8> = Vec::new();
    let mut file = (File::open(path).unwrap());
    file.read_to_end(&mut buffer).unwrap();
    buffer
}

